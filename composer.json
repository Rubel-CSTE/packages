{
    "name": "sufian/slug-helper",
    "description": "A Laravel package for generating unique slugs.",
    "type": "library",
    "authors": [
        {
            "name": "Sufian Techvill",
            "email": "sufian.techvill@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Sufian\\SlugHelper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sufian\\SlugHelper\\SlugHelperServiceProvider"
            ],
            "aliases": {
                "SlugHelper": "Suian\\SlugHelper\\SlugHelperFacade"
            }
        }
    }
}
